   <children>
      <VBox alignment="CENTER" prefHeight="250.0" prefWidth="320.0" spacing="30.0">
         <children>
            <Button onAction="#onActionDisplayAddAppointment" prefHeight="25.0" prefWidth="129.0" text="Add Appointment" />
            <Button layoutX="14.0" layoutY="98.0" onAction="#onActionDisplayUpdateAppointment" text="Update Appointment" />
            <Button layoutX="14.0" layoutY="110.0" onAction="#onActionDisplayDeleteAppointment" prefWidth="129.0" text="Delete Appointment" />
            <Button layoutX="14.0" layoutY="123.0" onAction="#onActionDisplayMainMenu" text="Back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
